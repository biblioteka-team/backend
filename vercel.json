
{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "redirects": [
      { "source": "/api", "destination": "/server.js", "permanent": true }
    ]
  }